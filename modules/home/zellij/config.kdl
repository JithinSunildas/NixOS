ui {
  status false
  pane_frames false
}

theme "kanagawa"
ui_mode "compact"
show_startup_tips false
default_shell "fish"
pane_frames true
auto_layout true
session_serialization false
copy_on_select true
scrollback_editor "nvim"
mirror_session false
mouse_mode true
scroll_buffer_size 10000
copy_command "wl-copy"
copy_clipboard "primary"

layout_dir "./layouts"
